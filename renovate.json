{
  "customManagers": [
    {
      "fileMatch": ["^Dockerfile$"],
      "matchString": "ENV .*?_VERSION=(?<currentValue>.*) # (?<datasource>.*?)/(?<depName>.*?)(\\&versioning=(?<versioning>.*?))?\\s"
    }
  ],
  "packageRules": [{
    "packageNames": "nodejs/node",
    "updateTypes": ["major"],
    "enabled": false
  }]
}
